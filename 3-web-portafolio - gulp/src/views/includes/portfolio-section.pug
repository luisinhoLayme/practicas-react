    .portfolio-section__title
      h2(data-heading="portfolio").portfolio-section__heading Latest Works
  //- portfolio filter start
  .portfolio-section__row
    #portfolio-filter.portfolio-filter
      span.portfolio-filter__item.portfolio-filter__item--active(data-target="all") all
      span.portfolio-filter__item(data-target="web-application") web application
      span.portfolio-filter__item(data-target="photoshop") photoshop
      span.portfolio-filter__item(data-target="mobile-app") mobile app
      span.portfolio-filter__item(data-target="e-commerce") e-commerce
