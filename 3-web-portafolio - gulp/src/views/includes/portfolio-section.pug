    //- portfolio item start
    .portfolio-item(data-category="web-application")
      .portfolio-item__inner
        .portfolio-item__img
          img(
            src="images/portfolio/thumb/project-1.png",
            alt="portfolio image",
            data-screenshots="images/portfolio/large/project-1/1.png, images/portfolio/large/project-1/2.png, images/portfolio/large/project-1/3.png, images/portfolio/large/project-1/4.png, images/portfolio/large/project-1/5.png, images/portfolio/large/project-1/6.png"
